<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="-45"
            android:pivotX="0%"
            android:pivotY="0%"
            android:toDegrees="0" >
            <shape android:shape="rectangle" >
                <solid android:color="#7aaef5" />

            </shape>
        </rotate>
    </item>
    <item android:left="12dp">
        <shape android:shape="rectangle" >
            <solid android:color="#7aaef5" />
            <corners android:radius="10dp"></corners>
        </shape>
    </item>
</layer-list>