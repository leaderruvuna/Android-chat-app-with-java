<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
   <item>
      <rotate
          android:fromDegrees="45"
          android:pivotX="100%"
          android:pivotY="0%"
          android:toDegrees="0" >
         <shape android:shape="rectangle" >
            <solid android:color="#fff" />

         </shape>
      </rotate>
   </item>
   <item android:right="12dp">
      <shape android:shape="rectangle" >
         <solid android:color="#fff" />
         <corners android:radius="10dp"></corners>
      </shape>
   </item>
</layer-list>